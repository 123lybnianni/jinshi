			var listdypjz = replaceAll($(".content-wrapper-div #dypjzlist").text(),"抵押品价值：",",").replace(",","");
			
			var dypxx = '{"loan_mortgage_name":"' + listdypxx + '","loan_mortgage_type":"' + listdyplx + '","loan_mortgage_bal":"' + listdypjz + '"}';
			params.custInfo = {
					"cust_id":"0104754535",						//客户号
					"cust_name":"对公测试客户8",				//客户名称
					"cust_branch_name":".Q联社.B县社.H分社",	//机构名称
					"cust_scale_code":"14",						//客户规模
					"cust_industry":"C0000",					//客户行业名称
					"cust_credit_level":"AAA",					//客户信用等级
					"loan_currency":"CNY",						//币种
					"loan_product_id":"427",					//产品名称
					"loan_bal":"6666",							//授信金额
					"loan_termdetail":"6",						//期限
					"loan_interest_id":"GDLL",					//利率类型
					"loan_value_date":"2017-03-31",				//起息日
					"loan_repayment_type":"DQYCHBFX",				//还款方式
					"loan_repayment_frequency":"1",				//还款频率
					"loan_termdetailtype":"M",					//期限单位
					"el_calcute_type":"PDLGD",					//风险成本计算方式
					//"extension_bal":"66666",					//授信金额
