		}
		$(".xinzen-btn").click(function(){
			if($(".xinzen-btnt-div").css("display")=="none"){
				$("#dypxx").val("");
				$("#dyplx").val("");
				$("#dypjz").val("");
				$(".xinzen-btnt-div").show();
			}else{
				$(".xinzen-btnt-div").hide();
			}
		});
		$("#khh").click(function(){
			window.open('kehuhao.html?parent="daikuanshisuan"','_parent');
		});
	});

	$("#dypConfirm").click(function(){
		$(".xinzen-btnt-div").hide();
